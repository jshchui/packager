{
  "name": "packager",
  "version": "1.0.0",
  "description": "An application that automates packaging tasks.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-test": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds [electron]",
    "minify": "grunt uglify:t2 && grunt cssmin:t1",
    "test": "jest"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-packager": "^14.2.1",
    "jest": "^25.1.0"
  },
  "dependencies": {
    "@node-minify/core": "^4.1.2",
    "@node-minify/cssnano": "^4.1.2",
    "@node-minify/uglify-js": "^4.1.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fix-path": "^2.1.0",
    "html2canvas": "^1.0.0-rc.5",
    "image-type": "^4.1.0",
    "kraken": "^0.4.0",
    "minify": "^4.1.3",
    "read-chunk": "^3.2.0",
    "socket.io": "^2.3.0"
  }
}
